{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "frontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/assets/css/socicon.css",
        "../node_modules/bootstrap/assets/css/entypo.css",
        "../node_modules/bootstrap/assets/css/theme.css"

      ],
      "scripts": [
          "../node_modules/bootstrap/jquery/jquery-3.2.1.slim.min.js",
          "../node_modules/bootstrap/cdnjs/ajax/libs/popper.js/1.13.0/umd/popper.min.js",
          "../node_modules/bootstrap/assets/js/jquery.smartWizard.min.js",
          "../node_modules/bootstrap/cdnjs/ajax/libs/flickity/2.0.10/flickity.pkgd.min.js",
          "../node_modules/bootstrap/assets/js/scrollMonitor.js",
          "../node_modules/bootstrap/cdnjs/ajax/libs/smooth-scroll/12.1.5/js/smooth-scroll.polyfills.min.js",
          "../node_modules/bootstrap/cdnjs/ajax/libs/prism/1.10.0/prism.min.js",
          "../node_modules/bootstrap/assets/js/zoom.min.js",
          "../node_modules/bootstrap/cdnjs/ajax/libs/twitter-bootstrap/4.0.0/js/bootstrap.min.js",
          "../node_modules/bootstrap/assets/js/theme.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
